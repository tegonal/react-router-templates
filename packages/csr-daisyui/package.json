    "i18next-http-backend": "^3.0.2",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-i18next": "^15.5.3",
    "react-router": "7.6.3",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@epic-web/config": "^1.21.0",
